    "@angular/compiler-cli": "*",
    "@angular/core": "*",
    "@angular/elements": "*",
    "@angular/forms": "*",
    "@angular/http": "*",
    "@angular/material": "*",
    "@angular/platform-browser": "*",
    "@angular/platform-browser-dynamic": "*",
    "@angular/platform-server": "*",
    "@angular/router": "*",
    "@babel/cli": "*",
    "@babel/plugin-proposal-decorators": "*",
