{
    "name": "angular-generic",
    "version": "1.0.0",
    "description": "useful services, filters and directives for every project needs",
    "keywords": ["angular", "generic", "tools"],
    "files": ["index.coffee"],
    "repository": {
        "type": "git", "url": "https://github.com/thaibault/angular-generic"
    },
    "homepage": "http://torben.website/angular-generic",
    "main": "index.compiled.js",
    "webOptimizer": {"injection": {"externalAliases": {
        "jQuery-tools": "$.Tools"
    }}},
    "scripts": {
        "build": "webOptimizer build",
        "buildDLL": "webOptimizer buildDLL",
        "clear": "webOptimizer clear",
        "document": "webOptimizer document",
        "export": "webOptimizer build '{exportFormat:{external:\"var\",self:\"var\"}}'",
        "lint": "webOptimizer lint",
        "serve": "webOptimizer serve",
        "start": "npm run serve",
        "test": "webOptimizer test",
        "testInBrowser": "webOptimizer testInBrowser",
        "postinstall": "webOptimizer build",
        "preinstall": "webOptimizer preinstall",
        "watch": "webOptimizer build --watch",
        "watchDLL": "webOptimizer buildDLL --watch"
    },
    "license": "CC-BY-3.0",
    "dependencies": {
        "webOptimizer": "git+ssh://git@github.com/thaibault/webOptimizer.git",
        "jQuery-tools": "git+ssh://git@github.com/thaibault/jQuery-tools.git"
    },
    "devDependencies": {
        "jsdom": ">=9.4.1",
        "qunitjs": ">=2.0.0",
        "qunit-cli": ">=0.2.0*",
        "source-map-support": ">=0.4.2"
    }
}
