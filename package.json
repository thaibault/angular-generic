    "weboptimizer": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*",
    "zone.js": "~0.9.1"
  },
  "sideEffects": false,
  "webOptimizer": {
    "injection": {
      "entry": {
        "index": []
      },
      "external": {
        "#": "Local references should point to the pre-compiled ones.",
        "aliases": {
          "./animation": {
            "default": "./animation.compiled",
            "root": "angularGenericAnimation"
          },
          "./basePipe": {
            "default": "./basePipe.compiled",
            "root": "angularGenericBasePipe"
          },
          "./baseService": {
            "default": "./baseService.compiled",
            "root": "angularGenericBaseService"
          },
          "./component": {
            "default": "./component.compiled",
            "root": "angularGenericComponent"
          },
          "./editor": {
            "default": "./editor.compiled",
            "root": "angularGenericEditor"
          },
          "./pipe": {
            "default": "./pipe.compiled",
            "root": "angularGenericPipe"
          },
          "./service": {
            "default": "./service.compiled",
            "root": "angularGenericService"
          }
        }
      }
    },
    "module": {
      "aliases": {
        "#": "We need blob-util in node environments only.",
        "blob-util$": {
          "__evaluate__": "self.targetTechnology === 'node' ? 'clientnode' : 'blob-util/dist/blob-util.min.js'"
        },
        "#0": "We need to process this file dynamically when testing different environments.",
        "clientnode/test$": "clientnode/test",
        "^hammerjs$": "hammerjs/hammer.min.js",
        "jquery$": "jquery/dist/jquery.min.js",
        "#1": "Standard import is \"node-fetch/lib/index.mjs\" which has import declarations which aren't allowed for javascript module files.",
        "node-fetch$": "node-fetch/lib/index.js",
        "#2": "Avoid using default buggy \"pouchdb-promise/lib/index.es.js\" version.",
        "pouchdb-promise$": "pouchdb-promise/lib/index.js"
      },
      "optimizer": {
        "babelMinify": {
          "bundle": {
            "#": "Mangeling here would result in undefined variable (for injected service in first defined injectable). Example traceback in application's pre-renderer: ReferenceError: e is not defined   at Module.<anonymous> (/build/preRender.js:124780:25463)   at __webpack_require__ (/build/preRender.js:124780:694)   at Object.<anonymous> (/build/preRender.js:124780:3665)   at __webpack_require__ (/build/preRender.js:124780:694)   at module.exports.wrapperApi (/build/preRender.js:124780:2020)   at Object.<anonymous> (/build/preRender.js:124780:2066)   at Object.<anonymous> (/build/preRender.js:125381:30)   at __webpack_require__ (/build/preRender.js:23:30)   at Module.<anonymous> (/build/preRender.js:155089:26)   at __webpack_require__ (/build/preRender.js:23:30)",
            "__evaluate__": "self.debug ? null : {transform: {mangle: false}}"
          },
          "#": "This loader expects javascript (not typescript) here.",
          "module": null
        }
      },
      "preprocessor": {
        "javaScript": {
          "options": {
            "plugins": {
              "__prepend__": [
                "angular2-annotations",
                [
                  "@babel/plugin-proposal-decorators",
                  {
                    "legacy": true
                  }
                ],
                [
                  "@babel/plugin-proposal-class-properties",
                  {
                    "loose": true
                  }
                ]
              ],
              "__remove__": [
                "@babel/plugin-proposal-class-properties",
                "@babel/plugin-transform-runtime"
              ]
            }
          }
        }
      },
      "replacements": {
        "context": [
          [
            "/@angular\\/+core\\/+f?esm(?:201)?5/",
            {
              "__evaluate__": "`'${self.path.source.base}'`"
            }
          ]
        ]
      },
      "skipParseRegularExpressions": {
        "__evaluate__": "/(?:^|\\/)(?:(?:hammerjs\\/hammer)|(?:jquery\\/dist\\/jquery)|(?:pouchdb\\/dist\\/pouchdb))(?:\\.min)?\\.js(?:\\?.+|$)/"
      }
    },
    "target:basePipe": {
      "injection": {
        "entry": {
          "basePipe": "./basePipe"
        },
        "external": {
          "implicit": {
            "pattern": {
              "exclude": {
                "__append__": {
                  "__evaluate__": "/^\\.\\/basePipe/"
                }
              }
            }
          }
        }
      }
    },
    "target:component": {
      "injection": {
        "entry": {
          "component": "./component"
        },
        "external": {
          "implicit": {
            "pattern": {
              "exclude": {
                "__append__": {
                  "__evaluate__": "/^\\.\\/component/"
                }
              }
            }
          }
        }
      }
    },
    "target:editor": {
      "injection": {
        "entry": {
          "editor": "./editor"
        },
        "external": {
          "implicit": {
            "pattern": {
              "exclude": {
                "__append__": {
                  "__evaluate__": "/^\\.\\/editor/"
                }
              }
            }
          }
        }
      }
    },
    "target:export": {
      "module": {
        "preprocessor": {
          "javaScript": {
            "options": {
              "plugins": {
                "__remove__": "@babel/plugin-transform-runtime"
              }
            }
          }
        }
      }
    },
    "target:main": {
      "injection": {
        "entry": {
          "index": {
            "__evaluate__": "(self.givenCommandLineArguments.length && ['build', 'export'].includes(self.givenCommandLineArguments[2]) ? [] : []).concat('reflect-metadata', 'zone.js', self.givenCommandLineArguments.length && ['serve', 'test:browser'].includes(self.givenCommandLineArguments[2]) ? `webpack-dev-server/client?http${self.development.server.cert ? 's' : ''}://0.0.0.0:${self.development.server.port}` : [], self.givenCommandLineArguments.length && ['test', 'test:browser'].includes(self.givenCommandLineArguments[2]) ? './test' : './index')"
          }
        },
        "external": {
          "implicit": {
            "pattern": {
              "exclude": {
                "__append__": [
                  {
                    "__evaluate__": "/^\\.\\/index/"
                  }
                ]
              }
            }
          }
        }
      }
    },
    "target:pipe": {
      "injection": {
        "entry": {
          "pipe": "./pipe"
        },
        "external": {
          "implicit": {
            "pattern": {
              "exclude": {
                "__append__": {
                  "__evaluate__": "/^\\.\\/pipe/"
                }
              }
            }
          }
        }
      }
    },
    "target:preRenderer": {
      "exportFormat": {
        "external": "commonjs2",
        "self": "commonjs2"
      },
      "injection": {
        "entry": {
          "preRender": "./preRender"
        }
      },
      "module": {
        "optimizer": {
          "babelMinify": {
            "bundle": {
              "transform": {
                "#": "To provide a generic logging mechanism we need this feature to be exported.",
                "removeConsole": false
              }
            },
            "module": {
              "#": "Mangeling here results in undefined variable (for injected service in first defined one: \"InitialDataService\") when using the pre-renderer in application specific ones. Example traceback in application's pre-renderer: ReferenceError: e is not defined   at Module.<anonymous> (/build/preRender.js:124780:25463)   at __webpack_require__ (/build/preRender.js:124780:694)   at Object.<anonymous> (/build/preRender.js:124780:3665)   at __webpack_require__ (/build/preRender.js:124780:694)   at module.exports.wrapperApi (/build/preRender.js:124780:2020)   at Object.<anonymous> (/build/preRender.js:124780:2066)   at Object.<anonymous> (/build/preRender.js:125381:30)   at __webpack_require__ (/build/preRender.js:23:30)   at Module.<anonymous> (/build/preRender.js:155089:26)   at __webpack_require__ (/build/preRender.js:23:30)",
              "mangle": false,
              "#0": "To provide a generic logging mechanism we need this feature to be exported.",
              "removeConsole": false
            }
          }
        }
      },
      "targetTechnology": "node"
    },
    "target:service": {
      "injection": {
        "entry": {
          "service": "./service"
        },
        "external": {
          "implicit": {
            "pattern": {
              "exclude": {
                "__append__": {
                  "__evaluate__": "/^\\.\\/service/"
                }
              }
            }
          }
        }
      }
    },
    "target:single": {
      "injection": {
        "external": {
          "implicit": {
            "pattern": {
              "#": "We remove local module references to support effective tree shaking for cases where only parts of this library should be used.",
              "exclude": {
                "__remove__": "__0__"
              },
              "include": {
                "__append__": {
                  "__evaluate__": "/^\\./"
                }
              }
            }
          }
        }
      }
    },
    "target:standalone": {
      "injection": {
        "entry": {
          "animation": "./animation",
          "baseService": "./baseService",
          "mockup": "./mockup",
          "moduleHelper": "./moduleHelper"
        }
      }
    },
    "target:testRunner": {
      "injection": {
        "entry": {
          "testRunner": "./testRunner"
        },
        "external": {
          "implicit": {
            "pattern": {
              "exclude": {
                "__append__": {
                  "__evaluate__": "/^\\.\\/testRunner/"
                }
              }
            }
          }
        }
      }
    }
  }
}
